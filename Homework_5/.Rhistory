head(dftrain)
summary(dftrain)
summary(dfeval)
str(dftrain)
str(dfeval)
plot_train <- dftrain %>%
gather(key = 'variable', value = 'value')
ggplot(plot_train) +
geom_histogram(aes(x=value, y = ..density..), bins=30) +
